{
  "name": "deadlock-antigravity",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply deadlock-tier-list-db --remote",
    "db:migrate:local": "wrangler d1 migrations apply deadlock-tier-list-db --local",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx src/db/seed.ts && wrangler d1 execute deadlock-tier-list-db --file seed.sql --remote",
    "seed:local": "tsx src/db/seed.ts && wrangler d1 execute deadlock-tier-list-db --file seed.sql --local"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.0",
    "framer-motion": "^12.23.25",
    "lucide": "^0.556.0",
    "lucide-react": "^0.556.0",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@cloudflare/workers-types": "^4.20260129.0",
    "@opennextjs/cloudflare": "^1.16.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "wrangler": "^4.61.0"
  }
}